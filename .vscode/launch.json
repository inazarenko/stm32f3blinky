{
    "version": "0.2.0",
    "configurations": [
        {
            "preLaunchTask": "${defaultBuildTask}",
            "type": "probe-rs-debug",
            "request": "launch",
            "name": "probe-rs: Flash+Debug",
            "cwd": "${workspaceFolder}",
            "connectUnderReset": true,
            "chip": "STM32F303VCTx",
            "flashingConfig": {
                "flashingEnabled": true,
                "haltAfterReset": false
            },
            "coreConfigs": [
                {
                    "coreIndex": 0,
                    "rttEnabled": true,
                    "programBinary": "./target/thumbv7em-none-eabihf/debug/${workspaceFolderBasename}",
                    "svdFile": "./svd/STM32F303.svd"
                }
            ]
        },
        {
            "name": "cortex-debug test",
            "preLaunchTask": "${defaultBuildTask}",
            "servertype": "openocd",
            "serverpath": "../openocd/bin/openocd",
            "type": "cortex-debug",
            "svdFile": "./svd/STM32F303.svd",
            "executable": "./target/thumbv7em-none-eabihf/debug/${workspaceFolderBasename}",
            "searchDir": ["../openocd/share/openocd/scripts"],
            "configFiles": ["interface/stlink.cfg", "board/stm32f3discovery.cfg"],
            "openOCDLaunchCommands": [],
            "gdbTarget": "localhost:3333",
            "request": "attach",
            ""
        }
    ]
}